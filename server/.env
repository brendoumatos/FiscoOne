PORT=3001
JWT_SECRET=development_secret_key_123

# Configuração de Banco de Dados (AZURE POSTGRESQL)
# Preencha com os dados do Portal Azure > Connection Strings

# Host: geralmente algo como "fiscoone-server.postgres.database.azure.com"
POSTGRES_HOST=fiscoone-server.database.windows.net

# User: formato "usuario" ou "usuario@servidor"
POSTGRES_USER=SEU_USUARIO_AQUI

# Password: a senha que você definiu ao criar o recurso
POSTGRES_PASSWORD=SUA_SENHA_AQUI

POSTGRES_PORT=5432
POSTGRES_DB=fiscoone_db
POSTGRES_SSL=true
